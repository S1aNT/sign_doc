<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.gplvote.signdoc"
    android:versionCode="8"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/signdoc"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        //android:debuggable="true" >

        <activity
            android:name="org.gplvote.signdoc.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.gplvote.signdoc.InitKey"></activity>
        <activity android:name="org.gplvote.signdoc.RegisterSign"></activity>
        <activity android:name="org.gplvote.signdoc.DoSign"></activity>
    </application>

</manifest>
